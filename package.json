    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^6.0.5",
    "@prisma/client": "^4.2.1",
    "@swc/helpers": "~0.3.3",
    "class-transformer": "^0.5.1",
