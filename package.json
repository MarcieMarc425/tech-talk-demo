{
  "name": "tech-talk-demo",
  "version": "0.0.0",
  "author": {
    "name": "Marc Tse",
    "email": "marctse@gmail.com"
  },
  "description": "A demo project for tech talk showcasing e2e type safety",
  "license": "MIT",
  "scripts": {
    "git:cleanup": "git fetch -p && for branch in `git for-each-ref --format '%(refname) %(upstream:track)' refs/heads | awk '$2 == \"[gone]\" {sub(\"refs/heads/\", \"\", $1); print $1}'`; do git branch -D $branch; done",
    "start": "nx serve",
    "start:all": "nx run-many --target=serve --all --parallel=3",
    "build": "nx build",
    "build:all": "nx run-many --target=build --all --parallel=3",
    "build:affected": "nx affected:build",
    "test": "nx test",
    "test:affected": "nx affected:test --parallel=5",
    "lint": "nx run-many --target=lint --all --parallel=3",
    "lint:affected": "nx affected:lint --parallel=5",
    "ci": "pnpm build:affected && pnpm lint:affected && pnpm test:affected",
    "prisma:studio": "pnpm dlx prisma studio",
    "prisma:generate": "prisma generate",
    "prisma:dev": "prisma db push",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:reset": "prisma migrate reset",
    "prisma:deploy": "prisma deploy",
    "prisma:migrate-dev": "prisma migrate dev",
    "migrate:status": "prisma migrate status",
    "commit": "cz",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "@anatine/zod-mock": "^3.5.6",
    "@anatine/zod-nestjs": "^1.7.3",
    "@anatine/zod-openapi": "^1.9.7",
    "@emotion/react": "11.10.0",
    "@emotion/server": "11.10.0",
